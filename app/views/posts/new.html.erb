  <!-- Page Content -->
    <% if flash[:notice] %>
      <div class="alert alert-danger"><%= flash[:notice] %></div>
    <% end %>
      <div class="container-fluid"> 
        <h1 class="mt-4" style="text-align: center;">PARKING</h1>
        <h3 class="mt-1" style="text-align: center;">On Tech</h3>
        <div class="posts-form">
          <%= form_with model: @post, local: true do |f| %>
            <div class="form-group">
              <%= f.label :client_name, '1.氏名をご記入ください' %>
              <%= f.text_field :client_name, class: 'form-control' %>
            </div>

            <div class="form-group">
              <%= f.label :email, '2.メールアドレスをご記入ください' %>
              <%= f.text_field :email, class: 'form-control' %>
            </div>

            <div class="form-group">
              <%= f.label :near_station, '3.最寄り駅をご記入ください' %>
              <%= f.text_field :near_station, class: 'form-control' %>
            </div>

            <div class="form-group">
              <%= f.label :parking_address, '4.駐車場住所をご記入ください' %>
              <%= f.text_field :parking_address, class: 'form-control' %>
            </div>

            <div class="form-group">
              <%= f.label :parking_present, '5.現在、駐車場運営の有無をお選びください', {class: 'form-label'} %>
              <div class="input-group mb-3" data-toggle="buttons">
                <label class="btn btn-outline-secondary active" style="width:50%">
                  <%= f.radio_button :parking_present, true, {checked: true} %> 有
                </label>
                <label class="btn btn-outline-secondary" style="width:50%">
                  <%= f.radio_button :parking_present, false, {} %> 無
                </label>
              </div>
            </div>

            <div class="form-group">
            <label class="form-label">6.運営期間をご記入ください</label>
              <div class="input-group mb-3">
                <%= f.select :year_period, options_for_select(0..99),  { :prompt => "選択してください" }, {class: 'custom-select'} %>
                <div class="input-group-append">
                  <span class="input-group-text">年</span>
                </div>
                <%= f.select :month_period, options_for_select(0..12), { :prompt => "選択してください" }, {class: 'custom-select'} %>
                <div class="input-group-append">
                  <span class="input-group-text">月</span>
                </div>
              </div>
            </div>

            <label for="station">7.中途解約金の有無と金額をご記入ください</label>
              <div class="input-group mb-3">
                <select class="custom-select">
                  <option value="1">有</option>
                  <option value="2">無</option>
                </select>
                <input type="text" class="form-control" aria-label="Amount (to the nearest dollar)">
                <div class="input-group-append">
                  <span class="input-group-text">円</span>
                </div>
              </div>

            <label for="station">6.現在の1ヶ月賃料をご記入ください</label>
              <div class="input-group mb-3">
                <div class="input-group-prepend">
                  <span class="input-group-text">¥</span>
                </div>
                <input type="text" class="form-control" aria-label="Amount (to the nearest dollar)">
                <div class="input-group-append">
                  <span class="input-group-text">円/月</span>
                </div>
              </div>

            <label for="management">7.税区分をお選びください</label>
              <div class="input-group mb-3">
                <select class="custom-select" id="inputGroupSelect04">
                  <option value="1">税込</option>
                  <option value="2">税抜</option>
                  <option value="3">非課税</option>
                </select>
              </div>

            <label for="management">8.現在の土地の状況をお選びください</label>
              <div class="input-group mb-3">
                <select class="custom-select" id="inputGroupSelect04">
                  <option value="1">更地</option>
                  <option value="2">アスファルト</option>
                  <option value="3">砂石</option>
                </select>
              </div>

            <label for="management">9.駐車可能な台数をご記入ください</label>
              <div class="input-group mb-3">
                <input type="text" class="form-control" id="basic-url">
                <div class="input-group-append">
                  <span class="input-group-text">台</span>
                </div>
              </div>

            <label for="management">10.駐車場の広さをご記入くださいい</label>
              <div class="input-group mb-3">
                <input type="text" class="form-control" id="basic-url">
                <div class="input-group-append" >
                  <select class="custom-select" id="inputGroupSelect04">
                    <option value="0">平米</option>
                    <option value="1">坪</option>
                  </select>
                </div>
              </div>
            <label for="management">11.利用期間をご記入ください</label>
              <div class="input-group mb-3">
                <div class="input-group-prepend">
                  <span class="input-group-text">開始</span>
                </div>
                <input type="date" class="form-control" id="start">
                <div class="input-group-prepend">
                  <span class="input-group-text">終了</span>
                </div>
                <input type="date" class="form-control" id="finish">
              </div>
            <label for="station">12.希望の1ヶ月賃料をご記入ください</label>
              <div class="input-group mb-3">
                <div class="input-group-prepend">
                  <span class="input-group-text">¥</span>
                </div>
                <input type="text" class="form-control" aria-label="Amount (to the nearest dollar)">
                <div class="input-group-append">
                  <span class="input-group-text">円/月</span>
                </div>
              </div>
            <label for="management">13.その他特記事項（ご相談・お悩みなど）</label>
            <div class="input-group">
              <textarea class="form-control" aria-label="With textarea"></textarea>
            </div>
            <div class="regist-form">
              <button class="btn btn-primary" type="submit" href="#">登録する</button>
            </div>
          <% end %>
        </div>
      </div>
    <!-- /#page-content-wrapper -->

  </div>
  <!-- /#wrapper -->

  <!-- Bootstrap core JavaScript -->

